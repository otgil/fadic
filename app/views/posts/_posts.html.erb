  <div id ="stars">
  	<div id="cards_container">
  		<div class="w3-row-padding w3-padding-16 w3-center" id="food">
  		  <% @posts.each do |w| %>
  			<div class="ft-recipe", id="card_<%= w.id %>">
  			<div class="ft-recipe__content">
  			  <header class="content__header">
  			    <div class="icon-box">
      				<img src="<%= w.star.company.logo %>" style="width:28px;height:28px"; />
      			</div>
      			<div class="block_box">
      			  <h5> <%= w.star.who %>(<%= w.star.company.company %>) </h5>
      			  <a class="link_detail">
      			    <h5> 날짜가 들어갈 것 </h5>
      			    <h2 class="recipe-title" > <%= w.title %>
      			  </a>
      			</div>
  			    <ul class="recipe-details">
                <li class="recipe-details-item time"><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-check2" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M13.854 3.646a.5.5 0 0 1 0 .708l-7 7a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L6.5 10.293l6.646-6.647a.5.5 0 0 1 .708 0z"/>
                </svg><i class="ion ion-ios-clock-outline"></i><span class="value">20</span><span class="title">Views</span></li>
                                <li class="recipe-details-item ingredients"><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-chat-text" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M2.678 11.894a1 1 0 0 1 .287.801 10.97 10.97 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8.06 8.06 0 0 0 8 14c3.996 0 7-2.807 7-6 0-3.192-3.004-6-7-6S1 4.808 1 8c0 1.468.617 2.83 1.678 3.894zm-.493 3.905a21.682 21.682 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a9.68 9.68 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7-3.582 7-8 7a9.06 9.06 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105z"/>
                  <path fill-rule="evenodd" d="M4 5.5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5zM4 8a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7A.5.5 0 0 1 4 8zm0 2.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 0 1h-4a.5.5 0 0 1-.5-.5z"/>
                </svg><i class="ion ion-ios-book-outline"></i><span class="value">5</span><span class="title">Comment</span></li>
                                <li class="recipe-details-item servings"><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-graph-up" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                  <path d="M0 0h1v16H0V0zm1 15h15v1H1v-1z"/>
                  <path fill-rule="evenodd" d="M14.39 4.312L10.041 9.75 7 6.707l-3.646 3.647-.708-.708L7 5.293 9.959 8.25l3.65-4.563.781.624z"/>
                  <path fill-rule="evenodd" d="M10 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 .5.5v4a.5.5 0 0 1-1 0V4h-3.5a.5.5 0 0 1-.5-.5z"/>
                </svg><i class="ion ion-ios-person-outline"></i><span class="value">4-6</span><span class="title">Rank</span></li>
                            </ul>
  			<!-- 벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽!!!!!!!!-->
  
  

  
  
  			<!-- 벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽벽!!!!!!!!!-->
  
  
  
  
  		 <p class="description">
  			    <%= w.content %> 
  		 </p>
  			  
  
  
  
  		 <div class="foot">
  			  <footer class="content__footer"  style="
  				  margin-left: 20px;
  				  margin-right: 5px;
  				  float: left;">
  				 <a href="#">source</a></footer>
  
  			  <footer class="content__footer" style="
  				  margin-left: 5px;
  				  margin-right: 20px;
  				  float: right;"
  					  ><a href="/posts/<%= w.id %>">detail</a></footer>
  
  			  </div>
  
  				</div>
  		  </div>
  		  <% end %>
  		  </div>
  	   </div>
  </div>	   
  